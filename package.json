{
  "name": "helmes-github-search",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@uirouter/react": "^0.7.0",
    "classnames": "^2.2.6",
    "node-sass-chokidar": "^1.3.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts-ts": "3.1.0",
    "reflect-metadata": "^0.1.12",
    "superagent": "^4.0.0-beta.5"
